<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csrc.mapper.ReadXmlMapper">

  <insert id="importIntoDb" parameterType="java.util.List">
    INSERT INTO attributeNode(serviceNo,serviceName,serviceTitle,attributeNodeName,attributeNodeTitle,attributeNodeType,xmlName,attributeNodeMode,projectName,workspaceName)
    VALUES
    <foreach collection ="list" item="node" index= "index" separator =",">
      (#{node.serviceNo},#{node.serviceName},#{node.serviceTitle},#{node.attributeNodeName},#{node.attributeTitle},#{node.attributeType},#{node.xmlName},#{node.attributeMode},#{node.projectName},#{node.workspaceName})
    </foreach>
  </insert>

  <delete id="deleteAll" >
    delete from attributeNode where 1=1
  </delete>

</mapper>